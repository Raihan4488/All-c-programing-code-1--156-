#include <stdio.h>
#include <string.h>

int main() {
    char str1[100], str2[100];

    printf("Enter first string: ");
    fgets(str1, sizeof(str1), stdin);
    str1[strcspn(str1, "\n")] = '\0';

    printf("Enter second string: ");
    fgets(str2, sizeof(str2), stdin);
    str2[strcspn(str2, "\n")] = '\0';

    int result = strcmp(str1, str2);  // strcmp performs lexicographical comparison

    if (result == 0)
        printf("The strings are equal.\n");
    else if (result < 0)
        printf("First string comes before the second string lexicographically.\n");
    else
        printf("First string comes after the second string lexicographically.\n");

    return 0;
}
