#include <stdio.h>
#include <math.h>

int main() {
    int lower, upper, num, originalNum, remainder, sum, digits;

    printf("Enter lower bound: ");
    scanf("%d", &lower);
    printf("Enter upper bound: ");
    scanf("%d", &upper);

    printf("Armstrong numbers between %d and %d are:\n", lower, upper);

    for (num = lower; num <= upper; num++) {
        originalNum = num;
        sum = 0;
        digits = 0;

        // Count digits
        int temp = num;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }

        // Calculate sum of digits raised to 'digits' power
        temp = num;
        while (temp != 0) {
            remainder = temp % 10;
            sum += pow(remainder, digits);
            temp /= 10;
        }

        if (sum == originalNum)
            printf("%d ", num);
    }

    printf("\n");
    return 0;
}
