#include <stdio.h>

int main() {
    int r, c;

    // Input matrix size
    printf("Enter number of rows: ");
    scanf("%d", &r);
    printf("Enter number of columns: ");
    scanf("%d", &c);

    int A[r][c];

    // Input matrix elements
    printf("Enter the elements of the matrix:\n");
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            scanf("%d", &A[i][j]);
        }
    }

    // Swap first and last element of each row
    for (int i = 0; i < r; i++) {
        int temp = A[i][0];
        A[i][0] = A[i][c - 1];
        A[i][c - 1] = temp;
    }

    // Output modified matrix
    printf("\nMatrix after interchanging first and last elements of each row:\n");
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            printf("%d ", A[i][j]);
        }
        printf("\n");
    }

    return 0;
}
