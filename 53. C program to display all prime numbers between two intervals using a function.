#include <stdio.h>

// Function to check prime
int isPrime(int n) {
    if (n <= 1)
        return 0;

    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0)
            return 0;
    }
    return 1;
}

// Function to display primes in a range
void displayPrimes(int start, int end) {
    printf("Prime numbers between %d and %d are:\n", start, end);

    for (int i = start; i <= end; i++) {
        if (isPrime(i)) {
            printf("%d ", i);
        }
    }
    printf("\n");
}

int main() {
    int low, high;

    printf("Enter two intervals: ");
    scanf("%d %d", &low, &high);

    displayPrimes(low, high);

    return 0;
}
