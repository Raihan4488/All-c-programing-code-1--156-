#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements in the sorted array: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter %d elements (sorted order):\n", n);
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    if (n == 0) {
        printf("Array is empty.\n");
        return 0;
    }

    int temp[n];  // Temporary array to store unique elements
    int j = 0;    // Index for temp array

    temp[j++] = arr[0];  // First element is always unique

    for (int i = 1; i < n; i++) {
        if (arr[i] != arr[i - 1]) {
            temp[j++] = arr[i];  // Store only unique elements
        }
    }

    // Print array without duplicates
    printf("Array after removing duplicates:\n");
    for (int i = 0; i < j; i++)
        printf("%d ", temp[i]);
    printf("\n");

    return 0;
}
