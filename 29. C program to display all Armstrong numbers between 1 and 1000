#include <stdio.h>
#include <math.h>

int main() {
    int num, originalNum, remainder, sum, digits;

    printf("Armstrong numbers from 1 to 1000 are:\n");

    for (num = 1; num <= 1000; num++) {
        originalNum = num;
        sum = 0;
        digits = 0;

        // Count digits
        int temp = num;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }

        // Calculate sum of digits raised to 'digits' power
        temp = num;
        while (temp != 0) {
            remainder = temp % 10;
            sum += pow(remainder, digits);
            temp /= 10;
        }

        if (sum == originalNum)
            printf("%d ", num);
    }

    printf("\n");
    return 0;
}
