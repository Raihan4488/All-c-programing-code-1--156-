#include <stdio.h>

int main() {
    // Declare a 2D array of pointers (3 rows x 2 columns)
    char *arr[3][2] = {
        {"Hello", "World"},
        {"C", "Programming"},
        {"TwoD", "Pointers"}
    };

    // Print all elements
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 2; j++) {
            printf("arr[%d][%d] = %s\n", i, j, arr[i][j]);
        }
    }

    return 0;
}
